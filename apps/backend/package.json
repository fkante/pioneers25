{
	"name": "backend",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "tsx watch --clear-screen=false src/index.ts",
		"build": "tsc",
		"start": "node dist/index.js",
		"lint": "eslint src --ext .ts",
		"format": "prettier --write \"src/**/*.ts\"",
		"check": "prettier --write . && eslint --fix",
		"test:agents": "tsx src/tests/create-two-agents.ts"
	},
	"dependencies": {
		"@elevenlabs/elevenlabs-js": "^2.24.1",
		"@t3-oss/env-core": "^0.13.8",
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"helmet": "^8.0.0",
		"morgan": "^1.10.0",
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/morgan": "^1.9.10",
		"@types/node": "^22.10.2",
		"prettier": "^3.5.3",
		"tsx": "^4.20.1",
		"typescript": "^5.7.2"
	},
	"engines": {
		"node": ">=24.2.0",
		"pnpm": ">=10.11.0"
	}
}